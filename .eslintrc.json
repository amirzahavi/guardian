{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap": "off",
        "arrow-parens":"off",
        "comma-dangle": "off",
        "max-len": ["warn", {"code": 100}],
        "require-jsdoc": "off",
        "no-unused-vars": "off"
    }
}